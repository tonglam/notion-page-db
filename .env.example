# NotionPageDb Migration System Configuration
# Copy this file to .env and fill in your values

# Notion Configuration
NOTION_API_KEY=your-notion-api-key
NOTION_SOURCE_PAGE_ID=1ab7ef86-a5ad-81ab-a4cb-f8b8f37ec491
NOTION_TARGET_DATABASE_NAME=your-target-database-name
NOTION_RATE_LIMIT_DELAY=350

# AI Service Configuration
# You can configure multiple AI providers - uncomment the ones you want to use
# OpenAI Configuration
# AI_PROVIDER=openai
# AI_API_KEY=your-openai-api-key
# AI_MODEL=gpt-3.5-turbo
# AI_IMAGE_MODEL=dall-e-3
# AI_MAX_TOKENS=1000
# AI_TEMPERATURE=0.7

# DeepSeek Configuration
DEEPSEEK_API_KEY=your-deepseek-api-key

# Gemini Configuration
GEMINI_API_KEY=your-gemini-api-key

# DashScope Configuration 
DASHSCOPE_API_KEY=your-dashscope-api-key

# Storage Configuration - Cloudflare R2
# Note: You can use either STORAGE_* prefix or R2_* prefix for these variables
STORAGE_ACCOUNT_ID=your-r2-account-id
STORAGE_ACCESS_KEY_ID=your-r2-access-key-id
STORAGE_SECRET_ACCESS_KEY=your-r2-secret-access-key
STORAGE_BUCKET_NAME=your-r2-bucket-name
STORAGE_PUBLIC_URL=your-r2-public-url
STORAGE_REGION=auto
STORAGE_USE_PRESIGNED_URLS=false

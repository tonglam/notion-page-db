{
  "name": "notion-page-db",
  "version": "1.0.0",
  "description": "TypeScript system for migrating Notion pages to structured databases with AI enhancement",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "migrate": "node scripts/migrate.js",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "lint:md": "markdownlint-cli2 \"**/*.md\" \"!node_modules/**\"",
    "lint:md:fix": "markdownlint-cli2 --fix \"**/*.md\" \"!node_modules/**\""
  },
  "keywords": [
    "notion",
    "migration",
    "database",
    "portfolio",
    "technical-notes"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@aws-sdk/s3-request-presigner": "^3.758.0",
    "@notionhq/client": "^2.2.3",
    "axios": "^1.5.0",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1",
    "openai": "^4.86.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "markdownlint-cli2": "^0.17.2",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
